@model DualPrep.Models.MealViewModels.FavoriteData

@{
    ViewData["Title"] = "Favorite Meals";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>
    My Favorites
</h1>

@foreach (var item in Model.MealFavorites)
{
    <a asp-action="Details" asp-route-id="@item.Meal.Id">
        <p>@Html.DisplayFor(modelItem => item.Meal.Name)</p>
    </a>
}

